{
  "name": "Language Server E2E Test",
  "type": "extensionHost",
  "request": "launch",
  "runtimeExecutable": "${execPath}",
  "args": [
    "--extensionDevelopmentPath=${workspaceRoot}",
    "--extensionTestsPath=${workspaceRoot}/test/index",
    "${workspaceRoot}/test/testFixture"
  ],
  "outFiles": [
    "${workspaceRoot}/test/**/*.js"
  ]
}